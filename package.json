{
  "name": "jsunconf-logo-test",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "start": "npm run clean && npm run copy && parallelshell 'npm run budo' 'npm run stylus-watch' 'npm run static-watch'",
    "clean": "rm -rf build && mkdir build",
    "copy": "cp -vr src/*.html assets build/",
    "budo": "budo src/main.js:main.js --dir=build/ -- -t [babelify --presets es2015] -o build/main.js src/main.js",
    "stylus-watch": "stylus --watch --sourcemap-inline styles/main.styl -u autoprefixer-stylus -o build/main.css",
    "static-watch": "watch 'npm run copy' src/ assets/"
  },

  "author": "Martin Schuhfuss <m.schuhfuss@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "autoprefixer-stylus": "^0.8.1",
    "babelify": "^7.2.0",
    "babel-preset-es2015": "^6.1.18",
    "budo": "^7.0.0",
    "parallelshell": "^2.0.0",
    "stylus": "^0.52.4",
    "watch": "^0.16.0",
    "three": "^0.73.0"
  },
  "dependencies": {
  }
}
